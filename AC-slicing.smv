		next(consumer.allowedsNssais[2]) := case 
			(consumer.model_id = 0 & selectConsumer = 1) : consumer1.allowedsNssais[2];
			(consumer.model_id = 0 & selectConsumer = 2) : consumer2.allowedsNssais[2];
